<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyBankApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyBankApp"
    Shell.FlyoutBehavior="Disabled"
    Title="MyBankApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Logged In"
        ContentTemplate="{DataTemplate local:ClaimsView}"
        Route="ClaimsView" />

</Shell>
